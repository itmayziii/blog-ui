{
  "name": "blog-ui",
  "version": "0.0.1",
  "author": {
    "name": "Tommy May III",
    "email": "tommymay37@gmail.com",
    "url": "https://www.fullheapdeveloper.com"
  },
  "license": "MIT",
  "scripts": {
    "start:client": "ng serve --open=false -e=dev --host=www.fullheapdeveloper.local",
    "start:universal": "node dist/server.js",
    "install:submodules": "cd highlight.js && npm install",
    "build:highlight.js": "cd highlight.js && node tools/build.js -t browser xml javascript json typescript",
    "build:client": "npm run build:highlight.js && ng build -oh=all -e=prod --prod -aot=true",
    "build:client:local": "npm run build:highlight.js && ng build -oh=all -e=dev --prod --aot=true",
    "build:universal": "npm run build:client-and-server && npm run webpack:server",
    "build:universal:local": "npm run build:client-and-server:local && npm run webpack:server",
    "build:client-and-server": "npm run build:client && ng build -e=prod --prod --app 1 --output-hashing=false",
    "build:client-and-server:local": "npm run build:client:local && ng build -e=dev --prod --app 1 --output-hashing=false",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "test": "ng test --watch false",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.9",
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/core": "5.2.9",
    "@angular/forms": "5.2.9",
    "@angular/http": "5.2.9",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "@angular/platform-server": "5.2.9",
    "@angular/router": "5.2.9",
    "@nguniversal/common": "^5.0.0-beta.5",
    "@nguniversal/express-engine": "^5.0.0-beta.5",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "angular2-notifications": "^0.9.7",
    "bootstrap": "4.0.0-beta.2",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "intl": "^1.2.5",
    "marked": "^0.3.9",
    "ng2-validation": "^4.2.0",
    "preboot": "^6.0.0-beta.3",
    "rxjs": "^5.5.7",
    "ts-loader": "3.5.0",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.7.3",
    "@angular/compiler-cli": "5.2.9",
    "@angular/language-service": "5.2.9",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^8.0.30",
    "@types/marked": "^0.3.0",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "2.6.2"
  }
}
